                            <li class="footer-menu-item">
                                <i class="bi bi-arrow-right icon "></i><a class="footer-menu-link" asp-action="Contact" asp-controller="Home">İletişim</a>
                            </li>
                            <li class="footer-menu-item">
                                <i class="bi bi-arrow-right icon "></i><a class="footer-menu-link" asp-action="Services" asp-controller="Home">Hizmetler</a>
                            </li>
                            <li class="footer-menu-item">
                                <i class="bi bi-arrow-right icon "></i><a class="footer-menu-link"asp-action="Blog" asp-controller="Home">Blog</a>
                            </li>
                            <li class="footer-menu-item">
                                <i class="bi bi-arrow-right icon "></i><a class="footer-menu-link" asp-action="AboutUs" asp-controller="Home">Hakkımızda</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-12     col-lg-4 footer-col ">
                    <h2 class=" footer-col-title    ">İletişim Bilgileri</h2>
                    <div class="footer-col-content-wrapper">
                        <div class="contact-info-card"><i class="bi bi-envelope icon"></i><a class="text-lowercase  info" href="mailto:example@support.com">info@becktech.com.tr</a></div>
                        <div class="contact-info-card"><i class="bi bi-geo-alt icon"></i><span class="text-lowercase  info">Odtü Teknopark </span></div>
                        <!-- <div class="contact-info-card"><i class="bi bi-phone icon"></i><a class="info" href="tel:+20123456789">+20123456789  </a></div> -->
                        <div class="contact-info-card">
                            <div class="social-icons">
                                <div class="sc-wrapper dir-row sc-size-32">
                                    <ul class="sc-list">
                                        <li class="sc-item " title="Facebook"><a class="sc-link" href="#0" title="social media icon"><i class="fab fa-facebook-f sc-icon"></i></a></li>
                                        <li class="sc-item " title="youtube"><a class="sc-link" href="#0" title="social media icon"><i class="fab fa-youtube sc-icon"></i></a></li>
                                        <li class="sc-item " title="instagram"><a class="sc-link" href="#0" title="social media icon"><i class="fab fa-instagram sc-icon"></i></a></li>
                                        <li class="sc-item " title="twitter"><a class="sc-link" href="#0" title="social media icon"><i class="fab fa-twitter sc-icon"></i></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
